<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000" >

    <View
        android:id="@+id/imageAirIcon"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:contentDescription="@null"
        android:background="@drawable/main_car_setting_bg" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="113dp"
        android:background="@drawable/main_car_setting_btnset1_bg" >

        <ImageButton
            android:id="@+id/btnMainCarSettingCloseLamp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:background="@null"
            android:contentDescription="@null"
            android:src="@drawable/main_car_setting_btn_close_lamp_off" />

        <ImageButton
            android:id="@+id/btnMainCarSettingNearLamp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="-23dp"
            android:layout_marginRight="-23dp"
            android:background="@null"
            android:contentDescription="@null"
            android:src="@drawable/main_car_setting_btn_near_lamp_off" />

        <ImageButton
            android:id="@+id/btnMainCarSettingFarLamp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:contentDescription="@null"
            android:src="@drawable/main_car_setting_btn_far_lamp_off" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="4dp"
        android:background="@drawable/main_car_setting_btnset2_bg"
        android:orientation="vertical"
        android:visibility="visible" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="7dp" >

            <ImageButton
                android:id="@+id/btnMainCarSettingDoubleLamp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:background="@null"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_btn_fog_front_lamp_off" />

            <ImageButton
                android:id="@+id/btnMainCarSettingPositionLamp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="-11dp"
                android:layout_marginRight="-11dp"
                android:background="@null"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_btn_position_lamp_off" />

            <ImageButton
                android:id="@+id/btnMainCarSettingFogRearLamp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_btn_fog_lamp_off" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="-4dp" >

            <ImageButton
                android:id="@+id/btnMainCarSettingBackDoorLock"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="22dp"
                android:background="@null"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_btn_backdoor_lock_off" />

            <ImageButton
                android:id="@+id/btnMainCarSettingDoorLock"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="-10dp"
                android:layout_marginRight="-9dp"
                android:background="@null"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_btn_door_lock_off" />

            <ImageButton
                android:id="@+id/btnMainCarSettingAllWinOpen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_btn_win_opening_off" />
        </LinearLayout>
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="126dp" >

        <FrameLayout
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_gravity="left" >

            <ImageView
                android:id="@+id/main_car_setting_left_window_icon_up"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="40dp"
                android:layout_marginTop="10dp"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_left_window_icon_up" />

            <ImageView
                android:id="@+id/main_car_setting_left_window_icon_down"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="40dp"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_left_window_icon_down" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_gravity="right" >

            <ImageView
                android:id="@+id/main_car_setting_right_window_icon_up"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="10dp"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_right_window_icon_up" />

            <ImageView
                android:id="@+id/main_car_setting_right_window_icon_down"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="40dp"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_right_window_icon_down" />
        </FrameLayout>
    </FrameLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="376dp" >

        <FrameLayout
            android:id="@+id/frameLayout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:background="@drawable/main_car_setting_btn_left_window_bg" >

            <com.kandi.widget.PassThroughButton
                android:id="@+id/btnMainCarSettingLeftWinUp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/main_car_setting_btn_left_window_up_selector" />

            <com.kandi.widget.PassThroughButton
                android:id="@+id/btnMainCarSettingLeftWinDown"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_marginTop="94dp"
                android:background="@drawable/main_car_setting_btn_left_window_down_selector"
                android:gravity="right" />

            <ImageView
                android:id="@+id/iconMainCarSettingLeftWin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_btn_left_window_icon" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@drawable/main_car_setting_btn_right_window_bg" >

            <com.kandi.widget.PassThroughButton
                android:id="@+id/btnMainCarSettingRightWinUp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:background="@drawable/main_car_setting_btn_right_window_up_selector" />

            <com.kandi.widget.PassThroughButton
                android:id="@+id/btnMainCarSettingRightWinDown"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginTop="94dp"
                android:background="@drawable/main_car_setting_btn_right_window_down_selector"
                android:gravity="right" />

            <ImageView
                android:id="@+id/iconMainCarSettingRightWin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@null"
                android:src="@drawable/main_car_setting_btn_right_window_icon" />
        </FrameLayout>

        <include
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            layout="@layout/main_car_setting_effect2" />
    </RelativeLayout>

	<RelativeLayout
	    android:id="@+id/loadingView"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent"
	    android:layout_centerInParent="true"
	    android:background="#8F000000"
	    android:clickable="true" >

	    <ProgressBar
	        android:id="@+id/pbar"
	        style="@style/custProgress"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_centerInParent="true"
	        android:layout_marginTop="20dp" />

	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_below="@+id/pbar"
	        android:layout_centerHorizontal="true"
	        android:layout_marginTop="20dp"
	        android:text="@string/device_onloading"
	        android:textColor="@color/white"
	        android:textSize="20sp" />

	</RelativeLayout>
</RelativeLayout>